.slide {
  width: 750rpx;
  height: 340rpx;
}
.slide-image {
  width: 100%;
  height: 100%;
}
.category {
  display: flex;
  background-color: white;
  height: 180rpx;
  justify-content: space-around;
  align-items: center;
}
.category-item {
  width: 128rpx;
  height: 140rpx;
}
.floor {
  margin-top: 20rpx;
  background-color: white;
}
.floor-head {
  width: 750rpx;
  height: 59rpx;
}
.floor-head image {
  margin: 10rpx 0rpx 0rpx 10rpx;
  width: 100%;
  height: 100%;
}
.floor-body {
  display: flex;
  padding: 20rpx 16rpx 10rpx 16rpx;
}
.floor-body-l {
  width: 232rpx;
  height: 386rpx;
}
.floor-body-l image {
  width: 100%;
  height: 100%;
}
.floor-body-r {
  flex: 1;
  display: flex;
  flex-wrap: wrap;
}
.floor-body-r image {
  height: 188rpx;
  margin: 0 0 10rpx 10rpx;
}
.bottom {
  display: flex;
  align-items: center;
  justify-content: center;
  height: 100rpx;
}
.bottom .bottomline {
  font-size: 24rpx;
  color: #ccc;
}
.bottom .icon-xiao {
  margin-right: 10rpx;
  padding-top: 15rpx;
}
.gototop {
  width: 100rpx;
  height: 100rpx;
  border-radius: 50%;
  background-color: #ffffff;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  position: fixed;
  bottom: 20rpx;
  right: 30rpx;
}
.gototop .totop {
  width: 20rpx;
  height: 20rpx;
}
.gototop .toptext {
  font-size: 24rpx;
  color: #666;
}
